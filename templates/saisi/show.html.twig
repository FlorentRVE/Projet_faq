{% extends 'base.html.twig' %}

{% block title %}Saisi{% endblock %}

{% block body %}
    <h1>Saisi</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ saisi.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ saisi.date ? saisi.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Heure</th>
                <td>{{ saisi.heure ? saisi.heure|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ saisi.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ saisi.prenom }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ saisi.telephone }}</td>
            </tr>
            <tr>
                <th>Commentaire</th>
                <td>{{ saisi.commentaire }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_saisi_index') }}">back to list</a>

    <a href="{{ path('app_saisi_edit', {'id': saisi.id}) }}">edit</a>

    {{ include('saisi/_delete_form.html.twig') }}
{% endblock %}
